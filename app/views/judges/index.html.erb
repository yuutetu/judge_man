<h1>Listing judges</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Select type</th>
      <th>Judge type</th>
      <th>Judge time</th>
      <th>Remove time</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @judges.each do |judge| %>
    <tr>
      <td><%= judge.title %></td>
      <td><%= judge.select_type %></td>
      <td><%= judge.judge_type %></td>
      <td><%= judge.judge_time %></td>
      <td><%= judge.remove_time %></td>
      <td><%= link_to 'Show', judge %></td>
      <td><%= link_to 'Edit', edit_judge_path(judge) %></td>
      <td><%= link_to 'Destroy', judge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br />

<%= link_to 'New Judge', new_judge_path %>
